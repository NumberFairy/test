/*menu样式reset*/
.nav-header{
    padding:8px;
}
.admin-logo{
    max-width:100%;
}
/*查询form 样式reset*/
@media screen and (min-width: 768px) {
    .form-inline .label-reset{
         text-align:right;
    }
}
.form-inline .label-reset{
    min-width:82px;
}
.form-inline .select-reset,
.form-inline .input-reset
    {
    min-width:194px;
}

/*溢出显示省略号*/
.ellipsis{
    max-width:300px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}