package com.wisdombud.alumni.util;

import java.util.ArrayList;
import java.util.List;

public class RoleZTreeNode extends ZTreeNode {
    private Integer            sortFlag;
    private List<RoleZTreeNode> children = new ArrayList<>();

    public List<RoleZTreeNode> getChildren() {
        return children;
    }

    public void setChildren(List<RoleZTreeNode> children) {
        this.children = children;
    }

    public Integer getSortFlag() {
        return sortFlag;
    }

    public void setSortFlag(Integer sortFlag) {
        this.sortFlag = sortFlag;
    }

}
