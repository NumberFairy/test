<script>
$(document).ready(function() {
    $('body').delegate('.menu-item', 'mouseover mouseout', function(e) {
        if (e.type == 'mouseover') {
            $(this).addClass("hover").find(".menu-inner").stop(true, true).show();
        } else {
            $(this).removeClass("hover").find(".menu-inner").stop(true, true).hide();
        }
    })
});
</script>