package com.wisdombud.alumni.vo.echars;

import java.util.List;

/**
 * 功能: LegendVo.<br/>
 * date: 2015年4月9日 上午10:50:02 <br/>
 *
 * @author joseph
 * @author Even.
 * @version
 * @since JDK 1.7
 */
public class LegendVo {
    private String       orient; // 布局方式
    private String       x;     // 水平位置
    private String       y;     // 垂直位置
    private List<String> data;

    public List<String> getData() {
        return data;
    }

    public void setData(List<String> data) {
        this.data = data;
    }

    public String getOrient() {
        return orient;
    }

    public void setOrient(String orient) {
        this.orient = orient;
    }

    public String getX() {
        return x;
    }

    public void setX(String x) {
        this.x = x;
    }

    public String getY() {
        return y;
    }

    public void setY(String y) {
        this.y = y;
    }

}
